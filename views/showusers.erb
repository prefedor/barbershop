<h2> Clients  </h2>

<%= @results %>

<table class="table">

	<% @results.each do |row| %>
		<tr>
			<td> <%= row['username']%> </td>
			<td> <%= row['phone']%> </td>
			<td> <%= row['datestamp']%> </td>
			<td> <%= row['barber']%> </td>
			<td> <%= row['color']%> </td>

		</tr>

	 <% end %>




